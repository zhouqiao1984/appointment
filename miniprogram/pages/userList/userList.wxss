/* pages/userList/userList.wxss */
page {
  width: 100%;
}
.i-row {
  padding: 0 20rpx;
  /* color: #1607e0ec; */
  font-family: Cursive;
  font-size:  40rpx;
}

.i-text {
  /* color: #1607e0ec; */
  margin: 20rpx auto;
  padding: .5em 1em;
  border:solid 2px #FFD700;
  border-radius: 20rpx;
  /* line-height: 1em; */
  display: flex;
  display: -webkit-flex;
  background: #fff;
}
.i-left {
  width: 70%;
}
.i-right {
  padding: 1em 1em;
  width: 30%;
}
.i-card {
  border-radius: .5em;
  background: #FFD700;
  width: 4em;
  height: 4em;
  color: white;
  text-align: center;
  
}
.i-ele {
  display: flex;
  display: -webkit-flex;
  padding: .5em 0;
}
.i-text image {
  margin-right: 30rpx;
  width: 1em;
  height: 1em;
}

.i-nodata {
  font-family: Cursive;
  font-size:  80rpx;
  text-align: center;
  margin-top: 20rpx;
}