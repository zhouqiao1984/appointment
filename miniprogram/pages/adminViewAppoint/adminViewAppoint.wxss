/* pages/adminViewAppoint/adminViewAppoint.wxss */
page {
  width: 100%;
}
.item-head {
  font-family: Cursive;
  width: 100%;
}
.item-head image {
  margin-right: 10rpx; 
  width: 36rpx;
  height: 36rpx;
}
.item-body {
  margin: 5rpx 5rpx;
  padding: 20rpx 20rpx;
  border:solid 2px #EEB4B4;
  border-radius: 20rpx;
}
.item-row {
  /* margin: 5rpx auto; */
  display: flex;
  display: -webkit-flex;
}
.item-top {
  margin-top: 12rpx;
}
.free {
  border:solid 2px rgb(14, 223, 42);
}
.item-value {
  width: 20%;
}
.item-baby {
  width: 40%;
  display: flex;
  display: -webkit-flex;

}
.item-baby>view{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 80%;
}

.item-tel {
  display: flex;
  display: -webkit-flex;
}